{
    "name": "codelicia/xulieta",
    "description": "Xulieta is a light php binary that lint documentation snippets",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/codelicia/xulieta",
    "keywords": [
        "check documentation",
        "documentation",
        "linter"
    ],
    "authors": [
        {
            "name": "Jefersson Nathan",
            "email": "malukenho.dev@gmail.com"
        },
        {
            "name": "Alexandre Eher",
            "email": "alexandre@eher.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "Codelicia\\Xulieta\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4",
        "beberlei/assert": "^3.2",
        "doctrine/rst-parser": "^0.1.0",
        "nikic/php-parser": "^4.3",
        "ocramius/package-versions": "^1.5",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/process": "^5.0",
        "webmozart/assert": "^1.7"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.0.0",
        "infection/infection": "^0.16.3",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-master",
        "staabm/annotate-pull-request-from-checkstyle": "^1.1",
        "vimeo/psalm": "^3.9"
    },
    "scripts": {
        "changelog": "git log $(git describe --abbrev=0 --tags)...HEAD --no-merges --pretty=format:\"* [%h](http://github.com/codelicia/xulieta/commit/%H) %s (%cN)\""
    },
    "bin": [
        "bin/xulieta"
    ]
}
